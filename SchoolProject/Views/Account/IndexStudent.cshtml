@model IEnumerable<SchoolProject.Models.EditUserViewModel>

@{
    ViewBag.Title = "IndexStudent";
}

<h2>List of students</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.SureName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FirstName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BirthDate)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.SureName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BirthDate)
            </td>
            <td>
                @*@Html.ActionLink("Edit", "Edit", new { email = item.Email }) |*@
                @Html.ActionLink("Delete", "Delete", new {email = item.Email})
            </td>
        </tr>
    }

</table>
